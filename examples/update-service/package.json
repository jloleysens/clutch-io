{
  "name": "journey-update-server",
  "version": "0.0.1",
  "description": "Service for updating JourneyApps apps",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "tslint --fix --project .",
    "ts-mocha": "mocha --require ts-node/register",
    "test": "yarn ts-mocha --recursive \"test/*Spec.ts\"",
    "test:publish": "yarn ts-mocha --recursive \"test/publishSpec.ts\"",
    "test:check": "yarn ts-mocha --recursive \"test/checkForUpdateSpec.ts\"",
    "test:watch": "yarn ts-mocha --reporter spec --watch-extensions ts --watch \"./test/**/*Spec.ts\" \"./src/**/*.ts\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "boom": "7.2.0",
    "jsonwebtoken": "8.2.0",
    "koa": "2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "loglevel": "1.6.1",
    "mongodb": "^3.0.4",
    "p-retry": "^1.0.0"
  },
  "devDependencies": {
    "@types/boom": "^7.2.0",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.27",
    "@types/loglevel": "^1.5.3",
    "@types/mongodb": "^3.0.8",
    "prettier": "^1.11.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "2.7.2"
  }
}
